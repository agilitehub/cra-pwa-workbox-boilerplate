# Firebase Configuration
# Replace these values with your Firebase project configuration
REACT_APP_FIREBASE_API_KEY=your-api-key
REACT_APP_FIREBASE_AUTH_DOMAIN=your-project-id.firebaseapp.com
REACT_APP_FIREBASE_PROJECT_ID=your-project-id
REACT_APP_FIREBASE_STORAGE_BUCKET=your-project-id.appspot.com
REACT_APP_FIREBASE_MESSAGING_SENDER_ID=your-messaging-sender-id
REACT_APP_FIREBASE_APP_ID=your-app-id
REACT_APP_FIREBASE_MEASUREMENT_ID=your-measurement-id
REACT_APP_FIREBASE_VAPID_KEY=your-vapid-key

# Firebase Options
REACT_APP_FIREBASE_ENABLE_PERSISTENCE=true
REACT_APP_FIREBASE_ENABLE_OFFLINE=true
REACT_APP_FIREBASE_ENABLE_ANALYTICS=false

# API Endpoints
# Replace with your backend API endpoints
REACT_APP_API_ENDPOINT_TODOS=https://your-api.com/todos

# IndexedDB Configuration
REACT_APP_INDEXEDDB_NAME=pwa-boilerplate
REACT_APP_INDEXEDDB_VERSION=1

# PWA Configuration
REACT_APP_PWA_NAME=CRA PWA Workbox Boilerplate
REACT_APP_PWA_SHORT_NAME=PWA Boilerplate
REACT_APP_PWA_THEME_COLOR=#3B82F6
REACT_APP_PWA_BACKGROUND_COLOR=#F3F4F6

# Service Worker Configuration
REACT_APP_SW_UPDATE_CHECK_INTERVAL=60000 